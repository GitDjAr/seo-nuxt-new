{
  "$schema": "http://json.schemastore.org/web-types",
  "name": "project",
  "framework": "project",
  "version": "0.0.0",
  "contributions": {
    "html": {
      "elements": [
        {
          "name": "BaseIcon",
          "attributes": [
            {
              "name": "name",
              "customType": "icon",
              "placeholder": "i-line-md-iconify1"
            },
            {
              "name": "width",
              "customType": "select",
              "default": "24px",
              "options": [
                "12px",
                "16px",
                "20px",
                "24px",
                "28px",
                "32px",
                "36px"
              ]
            },
            {
              "name": "height",
              "customType": "select",
              "default": "24px",
              "options": [
                "12px",
                "16px",
                "20px",
                "24px",
                "28px",
                "32px",
                "36px"
              ]
            }
          ]
        },
        {
          "name": "BaseButton",
          "attributes": [
            {
              "name": "label",
              "placeholder": "My Button"
            },
            {
              "name": "size",
              "customType": "select",
              "default": "sm",
              "options": [
                "2xs",
                "xs",
                "sm",
                "md",
                "lg",
                "xl"
              ]
            },
            {
              "name": "variant",
              "customType": "select",
              "default": "solid",
              "options": [
                "solid",
                "outline",
                "soft",
                "ghost",
                "link"
              ]
            },
            {
              "name": "target",
              "customType": "select",
              "default": "",
              "options": [
                "_blank"
              ]
            }
          ],
          "slots": [
            {
              "name": "default",
              "description": "默认的 Vue 插槽。",
              "vue-properties": [
                {
                  "name": "disabled-demo",
                  "description": "无论按钮是否被禁用。请注意，这不是一个真正的插槽道具，添加到此处只是为了展示如何在 Web 类型中定义它的示例。",
                  "type": "Boolean"
                }
              ]
            },
            {
              "name": "leading",
              "description": "使用#leading 槽来设置前导图标的内容。"
            },
            {
              "name": "trailing",
              "description": "使用#trailing 槽来设置尾随图标的内容。"
            }
          ]
        }
      ],
      "attributes": [
        {
          "name": "color",
          "customType": "select",
          "options": []
        }
      ]
    }
  }
}
